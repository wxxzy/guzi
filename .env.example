# .env.example
# This file serves as a template for your .env file.
# Copy this file to .env and fill in your actual values.

# Gemini API Key (required for AI services)
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Development Database URL (for DuckDB)
# Example: duckdb:///guzi_dev.db (relative to project root)
DEV_DATABASE_URL=duckdb:///guzi_dev.db

# Flask configuration (optional)
FLASK_CONFIG=development
